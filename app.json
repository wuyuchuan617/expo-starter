{
  "expo": {
    "name": "thirdweb playground",
    "slug": "book",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.thirdweb.demo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thirdweb.demo",
      "associatedDomains": [
        "webcredentials:thirdweb.com",
        "applinks:thirdweb.com",
        "webcredentials:thirdweb.com",
        "applinks:thirdweb.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thirdweb.demo",
      "intentFilters": [
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "thirdweb.com"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          },
          "ios": {
            "extraPods": [
              {
                "name": "OpenSSL-Universal",
                "configurations": [
                  "Release",
                  "Debug"
                ],
                "modular_headers": true,
                "version": "3.3.2000"
              }
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "70b744c1-f6c9-48a4-8001-e9319a7c3473"
      }
    },
    "owner": "yuchuan617"
  }
}
